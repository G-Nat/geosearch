<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>		
	</meta>
	<sheets>
		<sGeneral>
			<ROOT>
				<TCEforms>
				<sheetTitle>LLL:EXT:geosearch/locallang.xml:flexform.sheets.general.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>							       	
					<template_file>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.general.template</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>tmpl,html</allowed>
								<max_size>500</max_size>
								<uploadfolder>uploads/tx_geosearch/</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedListStyle>width:180px</selectedListStyle>
							</config>
						</TCEforms>
					</template_file>	
					<css_file>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.general.css</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>css</allowed>
								<max_size>500</max_size>
								<uploadfolder>uploads/tx_geosearch/</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedListStyle>width:180px</selectedListStyle>
							</config>
						</TCEforms>
					</css_file>				
					<startingpoint>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.general.startingpoint</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>22</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</startingpoint>							
	    		</el>
	  		</ROOT>
    	</sGeneral>
		<sForm>
			<ROOT>
				<TCEforms>
				<sheetTitle>LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>			
					<zipComplete>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.zipComplete</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>					
					</zipComplete>
					<countries>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.countries</label>
							<config>
								<type>select</type>
							 	<foreign_table>static_countries</foreign_table>
							 	<size>6</size>
							 	<maxitems>20</maxitems>
							 	<minitems>0</minitems>
							</config>
						</TCEforms>
					</countries>  		 	
					<maxRadius>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.maxRadius</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</maxRadius>		
					<unit>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.unit</label>
							<config>
								<type>select</type>
								<items type="array">								
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.unit.km</numIndex>
										<numIndex index="1">km</numIndex>
									</numIndex>																
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:geosearch/locallang.xml:flexform.sheets.form.unit.miles</numIndex>
										<numIndex index="1">miles</numIndex>
									</numIndex>																				
								</items>
							</config>
						</TCEforms>
					</unit>										
	    		</el>
	  		</ROOT>
    	</sForm> 
		<sList>
			<ROOT>
				<TCEforms>
				<sheetTitle>LLL:EXT:geosearch/locallang.xml:flexform.sheets.list.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>							
					<limit>
						<TCEforms>
							<label>LLL:EXT:geosearch/locallang.xml:flexform.sheets.list.limit</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</limit>							
	    		</el>
	  		</ROOT>
    	</sList>
  	</sheets>
</T3DataStructure>
